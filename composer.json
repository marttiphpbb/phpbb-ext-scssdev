	"description": "Scss Dev Extension for phpBB 3.2",
	"homepage": "https://github.com/marttiphpbb/phpbb-ext-scssdev",
	"version": "0.3.0",
	"time": "2019-06-26",
	"keywords": ["phpbb", "extension", "template", "custom"],
	"license": "GPL-2.0",
	"authors": [{
			"name": "martti",
			"email": "info@martti.be",
			"homepage": "https://github.com/marttiphpbb/",
			"role": "Developer"
		}],
	"require": {
		"php": ">=7.1",
		"leafo/scssphp": "^0.7.7",
		"matthiasmullie/minify": "^1.3"
